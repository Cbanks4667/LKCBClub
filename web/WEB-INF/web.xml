<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LKCBDisplayBooksServlet</servlet-name>
        <servlet-class>club.admin.LKCBDisplayBooksServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LKCBAddBookServlet</servlet-name>
        <servlet-class>club.admin.LKCBAddBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LKCBDisplayBooksServlet</servlet-name>
        <url-pattern>/LKCBDisplayBooks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LKCBAddBookServlet</servlet-name>
        <url-pattern>/LKCBAddBook</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>LKCBIndex.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/LKCBError.jsp</location>
    </error-page>
</web-app>
